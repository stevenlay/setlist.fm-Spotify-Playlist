<h2>
    Artist: <%= artist %>
</h2>
<h3>
    Tour: <%= tour %>
</h3>
<p>
   <% data.setlist.forEach(function(sets) { %>
        <% var actualSet = sets.sets.set; %>
        <% if(actualSet.length > 0) { %>
            <ol>
            <%actualSet.forEach(function(songList) { %>
                <% if(songList.encore) { %>
                    <% songList['song'].forEach(function(song) { %>
                        <%   songList['song'].forEach(function(song) { %>
                             <li> <%=   song.name %> </li>
                         <%  }); %>
                    <% }); %>
                <% } else { %>    
           <% songList['song'].forEach(function(song) { %>
             <li><%= song.name %></li>
        <% }) %> 
    <% } %>
       <%     }); %>
     <%   } %>
           </ol>
        
   <% }); %>
</p>


